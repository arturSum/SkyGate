
        //dodajemy stany

        var exitData = [],

            liczbaWystapien = 0;


        for(var key=0; key<exitCounterProduct.length; key++){

            liczbaWystapien = exitCounterProduct[key];

            if(liczbaWystapien > 0){

                while(liczbaWystapien--){

                    exitData.push(key);

                }

            }


        }

        console.log('exit: ', exitData);


        //format ciag wyjsciowy